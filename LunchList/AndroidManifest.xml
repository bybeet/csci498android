<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="csci498.bybeet.lunchlist"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="15" />
    <supports-screens android:largeScreens="true" android:smallScreens="false" android:normalScreens="true"/>

    <application
        android:icon="@drawable/lunchlist"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LunchList"
            android:label="@string/title_activity_lunch_list" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DetailForm"></activity>
        <activity android:name=".EditPreference"></activity>
    </application>

</manifest>